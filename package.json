{
  "name": "@slashgear/gdpr-cookie-scanner",
  "version": "3.4.0",
  "description": "CLI tool to scan websites for GDPR cookie consent compliance",
  "keywords": [
    "compliance",
    "consent",
    "cookie",
    "gdpr",
    "playwright",
    "privacy",
    "rgpd",
    "scanner"
  ],
  "homepage": "https://github.com/Slashgear/gdpr-cookie-scanner#readme",
  "bugs": {
    "url": "https://github.com/Slashgear/gdpr-cookie-scanner/issues"
  },
  "license": "MIT",
  "author": "Antoine Caron (https://slashgear.github.io)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slashgear/gdpr-cookie-scanner.git"
  },
  "bin": {
    "gdpr-scan": "dist/cli.js"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsc && node scripts/copy-data.mjs",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "lint": "oxlint .",
    "lint:fix": "oxlint --fix .",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "changeset": "changeset",
    "update-trackers": "node --experimental-strip-types scripts/update-trackers.ts",
    "update-trackers:dry": "node --experimental-strip-types scripts/update-trackers.ts --dry-run",
    "update:ocd": "node scripts/update-cookie-db.mjs"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "marked": "^17.0.3",
    "nanospinner": "^1.2.2",
    "playwright": "^1.49.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^22.0.0",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.48.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
